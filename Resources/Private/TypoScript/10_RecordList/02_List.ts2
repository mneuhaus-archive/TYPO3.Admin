########################################
# TYPO3.Expose:RecordList.List
# --------------------------------------
# Display a list of records as List
########################################

prototype(TYPO3.Expose:RecordList.List) < prototype(TYPO3.TypoScript:CollectionRenderer) {
	collection = ${objects}
	itemName = 'item'
	itemRenderer = TYPO3.Expose:RecordList.List.Element
}